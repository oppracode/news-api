{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "plugins": ["react", "react-native"],
  "env": {
    "browser": true,
    "es6": true,
    "react-native/react-native": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "react/prop-types": "off"
  }
}
